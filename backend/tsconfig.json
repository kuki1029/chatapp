{
  "compilerOptions": {
    "target": "ES6",
    "outDir": "./build/",
    "module": "nodenext",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": ["es2018.asynciterable"]
  }
}
